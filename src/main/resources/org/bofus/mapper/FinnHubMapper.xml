<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.bofus.jstock.mapper.FinnHubMapper">

<resultMap id="exchangeCountryCodesMap" type="org.bofus.jstock.domain.ExchangeCountryCodes"/>

<select id="getUsExchangeCode" resultType="map" resultMap="exchangeCountryCodesMap" >
    select ID, EXCHANGE_CODE from jstock.exchange_country_codes ecc 
    WHERE  EXCHANGE_CODE = 'US'; 
</select>
</mapper>